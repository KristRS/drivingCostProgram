// tripCosts.cpp  (Branch: fix-main) - Corrected main; no function yet
#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    float milesPerGallon;
    float dollarsPerGallon;

    // Read inputs: mpg then dollars per gallon
    cin >> milesPerGallon;
    cin >> dollarsPerGallon;

    // Compute costs inline (no function yet)
    float cost10  = (10.0f  / milesPerGallon) * dollarsPerGallon;
    float cost50  = (50.0f  / milesPerGallon) * dollarsPerGallon;
    float cost400 = (400.0f / milesPerGallon) * dollarsPerGallon;

    cout << fixed << setprecision(2)
         << cost10 << " " << cost50 << " " << cost400 << endl;
    return 0;
}
